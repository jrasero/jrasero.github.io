	author = {Gianaros, Peter J and Kraynak, Thomas E and Kuan, Dora C-H and Gross, James J and McRae, Kateri and Hariri, Ahmad R and Manuck, Stephen B and Rasero, Javier and Verstynen, Timothy D},
